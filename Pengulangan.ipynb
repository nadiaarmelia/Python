   ],
   "source": [
    "N = int(input(\"Banyaknya suku\"))\n",
    "total=0\n",
    "print(\"for: \")\n",
    "for i in range(1,N):\n",
    "    total+=i\n",
    "    print(i,\" + \",end=\" \")\n",
    "total+=N\n",
    "print(N,\"=\",total)\n",
    "\n",
    "print(\"while: \")\n",
    "i=1;total=0\n",
    "while i<N:\n",
    "    total+=i\n",
    "    print(i,\" + \",end=\" \")\n",
    "    i+=1\n",
    "total+=i\n",
    "print(i,\"=\",total)"
   ]
